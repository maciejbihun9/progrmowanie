
 * Load the data from json file to the application:
 - rows = [];

     constructor() {
       this.fetch((data) => {
         this.rows = data;
       });
     }

     fetch(cb) {
       const req = new XMLHttpRequest();
       req.open('GET', `assets/data/100k.json`);

       req.onload = () => {
         cb(JSON.parse(req.response));
       };

       req.send();
     }